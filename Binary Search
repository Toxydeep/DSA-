// Binary Search
#include<bits/stdc++.h>
using namespace std;
int binary_s(int arr[],int size1,int k){
int start=0;
int last=size1-1;
while(start<size1){

   int mid=start+(last-start/2);
   if(arr[mid]==k){
    return mid;
   }

  if(arr[mid]<k){
    start=mid+1;

  }
  else{
    last=mid-1;

  }
    mid=(start+last)/2;

}
return -1;






}

int main(){
int even[6]={2,4,6,8,10,12};
int odd[5]={1,3,5,7,9};
int e=binary_s(even,6,67);
int o=binary_s(odd,5,9);
cout<<e<<o;


}
