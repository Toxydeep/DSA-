//Palindrome
int n=124;
int rev=0;
int org=n;
while(n!=0){
    int r=n%10;
    rev=rev*10+r;
     n=n/10;
}
cout<<rev;
if(org==rev){
    cout<<"this number is Palindrome ";
}
else{
   cout<<" not Palindrome ";

}









