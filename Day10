//Day

#include <bits/stdc++.h>
using namespace std;

int print(int arr[],int n){
for(int i=0;i<n;i++){
    cout<<arr[i];
}
}

void swap1(int arr[],int n){
 for(int i=0;i<n;i+=2){
    if(i+1<n){
        swap(arr[i],arr[i+1]);

    }
 }


}


int main(){

int even[6]={2,4,5,6,8,5};
int odd[7]={1,2,6,3,4,7,8};
swap1(even,6);
print(even ,6);

return 0;

}
